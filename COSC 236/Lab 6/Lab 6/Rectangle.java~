public class Rectangle {
  int x;
  int y;
  int width;
  int height;
  
  public Rectangle(int x1, int y1, int width,int height){
    
    x = x1;
    y = y1;
      
    
    
    
  }
  
  public int getX(){
    return x;
  }
  public int getY(){
    return y;
  }
  public  int getWidth(){
    return width;
  }
  public int getHeight(){
    return height;
  }
  public String toString() {
    return "Rectangle[x="+x+",y ="+y+",width ="+width+", height = " +height+;
    
  }
  
  
  
  
  
}